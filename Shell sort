#include(stdio.h)

int main()
{
	int L[20],temp,a,b,n=6,m;
	printf("pengurutan berdasarkan Shell sort\nMasukkan %d elemen: \n"n);
	for(a=0;a<=n;a++){
		scanf("%d",&L[i]);
		}
	
	printf("\nSebelum sorting : ");
	
	for (a=0;a<n;a++){
	
	printf("%d",L[a]);
	
	}
	
	
	for(m=n/2;m>0;m/=2){
	/*6 7 2 1===>2 7 6 1, 2 1 67//1 2 6 7, 1 2 6 7,1 2 6 7*/
		 for (b=m;b<n;b++){
			for (a=b-m;a>0;a-=m){
				if(L[a+m})=L[a])break;
				else{
					temp = L[a];
					L[a]=L[b+m];
					L[a+m] = temp;
					
				}
			}
		}
	}
		printf("\nSesudah sorting: );
	for(a=0;a<n;a++)
	{
	printf("%d,L[a]);}
	printf("\n");
	
	}
